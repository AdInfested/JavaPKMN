package lib;
import java.util.ArrayList;

import lib.employeeRegister.Employee;

// Default Constructor
public class Register {
	
	// ArrayList of Type Name
	private ArrayList<Name> register;
	private String Name;

	
	//Names should be added removed and retrieved here
	
	public String getName() {
		return Name;
	}
	
	public void setName(String Name) {
		this.Name = Name;
	}
	
	public void addName(Name f) {
		register.add(f);
	}
	
	public Name removeName(int pos) {
		return register.remove(pos);
	}
	
	//Retrieve Register Size 
	
	public int size() {
		return register.size();
	}
	
	//CLear all entries within
	
	public void clearRegister() {
		register.clear();
	}
	
	//Check Register is empty
	
	public boolean isRegisterEmpty() {
		return register.isEmpty();
	}
	
	//Search by family name
	
	//Count First name occurrences
	
	
}
